<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:KarateSemaphore.Phone">
    <SolidColorBrush x:Key="aka" Color="#FFFF1100" />
    <SolidColorBrush x:Key="ao" Color="#FF0687FF" />
    <SolidColorBrush x:Key="panelBackground" Color="#000000" />
    <SolidColorBrush x:Key="panelForeground" Color="#FFFFFF" />
    <FontFamily x:Key="NI7SEG">/Views/Ni7seg.ttf#NI7SEG</FontFamily>
    <Style TargetType="TextBlock" x:Key="panelText">
        <Setter Property="Foreground" Value="{StaticResource ResourceKey=panelForeground}" />
        <Setter Property="FontFamily" Value="{StaticResource ResourceKey=NI7SEG}" />
    </Style>
    <views:TimeDisplayConverter x:Key="TimeDisplayConverter" />
    <views:PenaltyToIntConverter x:Key="PenaltyToIntConverter" />
    <views:LTRConverter x:Key="LTRConverter" />
</ResourceDictionary>