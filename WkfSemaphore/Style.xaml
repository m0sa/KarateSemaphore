<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:WkfSemaphore="clr-namespace:WkfSemaphore">
    <WkfSemaphore:MinutesSecondsConverter x:Key="timeConverter" />
    <SolidColorBrush x:Key="aka" Color="#FFFF1100" />
    <SolidColorBrush x:Key="ao" Color="#FF0687FF" />
    <Style TargetType="Button">
        <Setter Property="Margin" Value="1" />
    </Style>
    <Style TargetType="Button" x:Key="CautionButtonStyle">
        <Setter Property="Background" Value="Yellow" />
        <Setter Property="Foreground" Value="Red" />
        <Setter Property="Margin" Value="1" />
    </Style>
</ResourceDictionary>